{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceRoot}/client/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },

    {
      "name": "Go Backend API",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "./go-backend"
    }
  ]
}
